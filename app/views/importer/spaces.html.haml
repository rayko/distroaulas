- title 'Import Spaces'

%p
  = link_to 'Download Template', '/import_templates/spaces.xls', :target => 'blank'

- semantic_form_for :spaces, :url => importer_upload_spaces_path, :html => {:multipart => true} do |f|
  - f.inputs do
    = f.input :file, :as => :file, :required => true
  - f.buttons do
    = f.commit_button 'Upload'